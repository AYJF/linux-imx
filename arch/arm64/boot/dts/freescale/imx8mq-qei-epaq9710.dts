/* HDMI Display Support */
&vpu {
	vpu-supply = <&sw1c_reg>;
	status = "okay";
};

&gpu3d {
	status = "okay";
};

&vpu_v4l2 {
	status = "okay";
};

&irqsteer {
	status = "okay";
};

&dcss {
	status = "okay";

	port@0 {
		dcss_out: endpoint {
			  remote-endpoint = <&hdmi_in>;
		};
	};
};

&hdmi {
	compatible = "cdn,imx8mq-hdmi";
	lane-mapping = <0xe4>;
	hdcp-config = <0x3>;
	status = "okay";
	port@1 {
		hdmi_in: endpoint {
			remote-endpoint = <&dcss_out>;
		};
	};
};
